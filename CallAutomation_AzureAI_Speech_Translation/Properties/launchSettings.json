{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5071"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "AcsConnectionString": "endpoint=<yourEndpoint>;accesskey=<yourAccessKey>",
        "AcsPhoneNumber": "<yourPhoneNumber>",
        "AzureAISpeechKey": "<yourAISpeechKey>",
        "AzureAISpeechRegion": "<yourAISpeechRegion>"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7200;http://localhost:5071"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:6013",
      "sslPort": 44368
    }
  }
}